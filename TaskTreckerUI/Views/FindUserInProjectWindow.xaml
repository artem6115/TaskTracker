<Window x:Class="TaskTrackerUI.Views.FindUserInProjectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TaskTrackerUI.Views"
        mc:Ignorable="d"
        Title="Выбор пользователя" Height="500" Width="600">
    <Grid>
        <StackPanel>
            <TextBox x:Name="Find_text" KeyDown="Text_changed"/>
            <ListBox x:Name="User_list"/>
        </StackPanel>
        <StackPanel VerticalAlignment="Bottom" Orientation="Horizontal">
            <Button Click="Chouse_btn" Content="Выбрать"/>
            <Button Click="Close" Content="Отмена"/>
        </StackPanel>
    </Grid>
</Window>
